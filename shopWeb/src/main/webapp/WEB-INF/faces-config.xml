<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://xmlns.jcp.org/xml/ns/javaee
        http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<application>

		<resource-bundle>
			<base-name>categoriastxt</base-name>
			<var>msgCategorias</var>
		</resource-bundle>

		<resource-bundle>
			<base-name>productostxt</base-name>
			<var>msgProductos</var>
		</resource-bundle>

		<resource-bundle>
			<base-name>listaspreciostxt</base-name>
			<var>msgListasprecios</var>
		</resource-bundle>

		<resource-bundle>
			<base-name>usuariostxt</base-name>
			<var>msgUsuarios</var>
		</resource-bundle>

		<resource-bundle>
			<base-name>preciostxt</base-name>
			<var>msgPrecios</var>
		</resource-bundle>

	</application>

</faces-config>